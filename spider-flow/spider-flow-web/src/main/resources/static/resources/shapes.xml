<shapes>
	<shape name="rectangle" title="开始抓取" w="100" h="100" d="M 10,10 L 90,10 L 90,90 L 10,90 Z">
		<background>
			<path>
				<move x="0" y="0"/>
				<line x="100" y="0" />
				<line x="100" y="100" />
				<line x="0" y="100" />
				<close/>
			</path>
		</background>
		<foreground>
			<stroke color="#000"/>
		</foreground>
	</shape>
	<shape name="parallelogram" title="定义变量" w="100" h="100" d="M 30,10 L 90,10 L 70,90 L 10,90 Z">
		<background>
			<path>
				<move x="20" y="20"/>
				<line x="100" y="20" />
				<line x="80" y="80" />
				<line x="0" y="80" />
				<close/>
			</path>
		</background>
		<foreground>
			<stroke color="#000"/>
		</foreground>
	</shape>
	<shape name="output" title="输出" w="100" h="100" d="M 0,20 L 100,20 L 100,80 L 0,80 L 0,20 M 10,20 L 10,80 M 90,20 L 90,80 Z">
		<background>
			<path>
				<move x="0" y="20"/>
				<line x="100" y="20" />
				<line x="100" y="80" />
				<line x="0" y="80" />
				<line x="0" y="20" />
				<move x="10" y="20"/>
				<line x="10" y="80" />
				<move x="90" y="20"/>
				<line x="90" y="80" />
				<close/>
			</path>
		</background>
		<foreground>
			<stroke color="#000"/>
		</foreground>
	</shape>
	<shape name="datasource" title="定义数据源" w="100" h="100" d="M 10,30 L 10,70 A 40,20,0,1,0,90,70 L 90 30 A 40,20,0,1,0,10,30 A 40,20,0,1,0,90,30">
		<background>
			<path>
				<move x="1" y="20"/>
				<line x="1" y="80"/>
				<arc rx="40" ry="15" sweep-flag="0" large-arc-flag="1" x-axis-rotation="0" x="99" y="80"/>
				<line x="99" y="20"/>
				<arc rx="40" ry="15" sweep-flag="0" large-arc-flag="1" x-axis-rotation="0" x="1" y="20"/>
				<arc rx="40" ry="15" sweep-flag="0" large-arc-flag="1" x-axis-rotation="0" x="99" y="20"/>
			</path>
		</background>
		<foreground>
			<stroke color="#000"/>
		</foreground>
	</shape>
	<shape name="executeSql" title="执行sql" w="100" h="100" d="M 10 10,L 90 10, L 90 90, L 10 90, L 10,10,M 20 20 ,L 80 20,L 80 80,L 20 80 Z">
		<background>
			<path>
				<move x="0" y="0"/>
				<line x="100" y="0" />
				<line x="100" y="100" />
				<line x="0" y="100" />
				<line x="0" y="0" />
				
				<move x="10" y="10"/>
				<line x="90" y="10" />
				<line x="90" y="90" />
				<line x="10" y="90" />
				<close/>
			</path>
		</background>
		<foreground>
			<stroke color="#000"/>
		</foreground>
	</shape>
	
	<shape name="function" title="执行函数" w="100" h="100" d="M 10, 50 a40,40 0 1,0 80,0 a40,40 0 1,0 -80,0 Z">
		<background>
			<ellipse x="10" y="10" w="80" h = "80"/>
		</background>
		<foreground>
			<stroke color="#000"/>
		</foreground>
	</shape>
	<shape name="start" w="100" h="100" hidden="1" default="1" title="开始">
		<background>
			<path>
				<roundrect x="8" y="20" w="90" h="60" arcsize="10"/>
			</path>
		</background>
		<foreground>
			<stroke color="#000"/>
		</foreground>
	</shape>
</shapes>
